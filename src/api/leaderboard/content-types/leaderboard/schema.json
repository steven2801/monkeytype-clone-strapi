{
  "kind": "collectionType",
  "collectionName": "leaderboards",
  "info": {
    "singularName": "leaderboard",
    "pluralName": "leaderboards",
    "displayName": "leaderboard",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "leaderboards"
    },
    "wpm": {
      "type": "integer",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "words",
        "sentences",
        "numbers"
      ],
      "default": "words",
      "required": false
    },
    "time": {
      "type": "integer",
      "required": true
    },
    "name": {
      "type": "string",
      "required": false,
      "minLength": 3,
      "maxLength": 18,
      "default": "guest"
    }
  }
}
